<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.MultipleAssetHeader">
 
        <inquiry>
            <title>Multiple Asset Header Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="agencyNumber" />
                        <field attributeName="documentNumber" />
                        <field attributeName="newCapitalAssetCreateCount" />
                        <field attributeName="nextCapitalAssetPaymentLineNumber" />
                        <field attributeName="organizationOwnerAccountNumber" />
                        <field attributeName="organizationOwnerChartOfAccountsCode" />
                        <field attributeName="representativeUniversalIdentifier" />
                        <field attributeName="startingCapitalAssetNumber" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Multiple Asset Header Lookup</title>

            <instructions>Look up a Multiple Asset Header</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="agencyNumber" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="newCapitalAssetCreateCount" required="false" />
                <lookupField attributeName="nextCapitalAssetPaymentLineNumber" required="false" />
                <lookupField attributeName="organizationOwnerAccountNumber" required="false" />
                <lookupField attributeName="organizationOwnerChartOfAccountsCode" required="false" />
                <lookupField attributeName="representativeUniversalIdentifier" required="false" />
                <lookupField attributeName="startingCapitalAssetNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="agencyNumber" />
                <field attributeName="documentNumber" />
                <field attributeName="newCapitalAssetCreateCount" />
                <field attributeName="nextCapitalAssetPaymentLineNumber" />
                <field attributeName="organizationOwnerAccountNumber" />
                <field attributeName="organizationOwnerChartOfAccountsCode" />
                <field attributeName="representativeUniversalIdentifier" />
                <field attributeName="startingCapitalAssetNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="agencyNumber" forceUppercase="true">
                <label>Agency Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="7" />
                </control>

                <summary>agencyNumber</summary>
                <description>agencyNumber description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="newCapitalAssetCreateCount" forceUppercase="true">
                <label>New Capital Asset Create Count</label>
                <shortLabel>Count</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>newCapitalAssetCreateCount</summary>
                <description>newCapitalAssetCreateCount description...</description>
			</attribute>

            <attribute name="nextCapitalAssetPaymentLineNumber" forceUppercase="true">
                <label>Next Capital Asset Payment Line Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>5</maxLength>
