			</attribute>

            <attribute name="organizationOwnerAccountNumber" forceUppercase="true">
                <label>Organization Owner Account Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>organizationOwnerAccountNumber</summary>
                <description>organizationOwnerAccountNumber description...</description>
			</attribute>

            <attribute name="organizationOwnerChartOfAccountsCode" forceUppercase="true">
                <label>Organization Owner Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>organizationOwnerChartOfAccountsCode</summary>
                <description>organizationOwnerChartOfAccountsCode description...</description>
			</attribute>

            <attribute name="representativeUniversalIdentifier" forceUppercase="true">
                <label>Representative Universal Identifier</label>
