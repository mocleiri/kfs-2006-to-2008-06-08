			</attribute>

            <attribute name="startingCapitalAssetNumber" forceUppercase="true">
                <label>Starting Capital Asset Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>

                <summary>startingCapitalAssetNumber</summary>
                <description>startingCapitalAssetNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
