<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AquisitionType">
 
        <inquiry>
            <title>Aquisition Type Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="acquisitionTypeCode" />
                        <field attributeName="acquisitionTypeName" />
                        <field attributeName="active" />
                        <field attributeName="incomeAssetObjectCode" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Aquisition Type Lookup</title>

            <instructions>Look up an Aquisition Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="acquisitionTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="acquisitionTypeCode" required="false" />
                <lookupField attributeName="acquisitionTypeName" required="false" />
                <lookupField attributeName="active" required="false" />
                <lookupField attributeName="incomeAssetObjectCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="acquisitionTypeCode" />
                <field attributeName="acquisitionTypeName" />
                <field attributeName="active" />
                <field attributeName="incomeAssetObjectCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="acquisitionTypeCode" forceUppercase="true">
                <label>Acquisition Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>acquisitionTypeCode</summary>
                <description>acquisitionTypeCode description...</description>
			</attribute>

            <attribute name="acquisitionTypeName" forceUppercase="true">
                <label>Acquisition Type Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>acquisitionTypeName</summary>
                <description>acquisitionTypeName description...</description>
			</attribute>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>active</summary>
                <description>active description...</description>
			</attribute>

            <attribute name="incomeAssetObjectCode" forceUppercase="true">
                <label>Income Asset Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>incomeAssetObjectCode</summary>
                <description>incomeAssetObjectCode description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
