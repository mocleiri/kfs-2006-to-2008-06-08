                <field attributeName="acquisitionTypeCode" />
                <field attributeName="capitalAssetDescription" />
                <field attributeName="capitalAssetTypeCode" />
                <field attributeName="capitalizationDate" />
                <field attributeName="conditionCode" />
                <field attributeName="documentNumber" />
