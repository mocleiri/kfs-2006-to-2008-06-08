                <lookupField attributeName="manufacturerModelNumber" required="false" />
                <lookupField attributeName="manufacturerName" required="false" />
                <lookupField attributeName="primaryDepreciationBaseAmount" required="false" />
                <lookupField attributeName="primaryDepreciationMethodCode" required="false" />
                <lookupField attributeName="totalCostAmount" required="false" />
