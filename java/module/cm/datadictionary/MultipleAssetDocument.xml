            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="acquisitionSourceName" required="false" />
                <lookupField attributeName="acquisitionTypeCode" required="false" />
                <lookupField attributeName="capitalAssetDescription" required="false" />
                <lookupField attributeName="capitalAssetTypeCode" required="false" />
                <lookupField attributeName="capitalizationDate" required="false" />
                <lookupField attributeName="conditionCode" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="financialDocumentNextLineNumber" required="false" />
