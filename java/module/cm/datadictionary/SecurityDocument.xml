<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.SecurityDocument">
 
        <inquiry>
            <title>Security Document Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="chartOfAccountsCode" />
                        <field attributeName="documentNumber" />
                        <field attributeName="organizationCode" />
                        <field attributeName="securityActionCode" />
                        <field attributeName="securityChartOfAccountsCode" />
                        <field attributeName="securityOrganizationCode" />
                        <field attributeName="securityTypeCode" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Security Document Lookup</title>

            <instructions>Look up a Security Document</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="organizationCode" required="false" />
                <lookupField attributeName="securityActionCode" required="false" />
                <lookupField attributeName="securityChartOfAccountsCode" required="false" />
                <lookupField attributeName="securityOrganizationCode" required="false" />
                <lookupField attributeName="securityTypeCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="documentNumber" />
                <field attributeName="organizationCode" />
                <field attributeName="securityActionCode" />
                <field attributeName="securityChartOfAccountsCode" />
                <field attributeName="securityOrganizationCode" />
                <field attributeName="securityTypeCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Organization Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>organizationCode</summary>
                <description>organizationCode description...</description>
			</attribute>

            <attribute name="securityActionCode" forceUppercase="true">
                <label>Security Action Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>securityActionCode</summary>
                <description>securityActionCode description...</description>
			</attribute>

            <attribute name="securityChartOfAccountsCode" forceUppercase="true">
                <label>Security Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>securityChartOfAccountsCode</summary>
                <description>securityChartOfAccountsCode description...</description>
			</attribute>

            <attribute name="securityOrganizationCode" forceUppercase="true">
                <label>Security Organization Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>securityOrganizationCode</summary>
                <description>securityOrganizationCode description...</description>
			</attribute>

            <attribute name="securityTypeCode" forceUppercase="true">
                <label>Security Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>securityTypeCode</summary>
                <description>securityTypeCode description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
