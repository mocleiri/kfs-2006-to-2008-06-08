<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.BankAccountType">
 
        <inquiry>
            <title>Bank Account Type Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="active" />
                        <field attributeName="customerBankAccountTypeCode" />
                        <field attributeName="customerBankAccountTypeName" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Bank Account Type Lookup</title>

            <instructions>Look up a Bank Account Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="customerBankAccountTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="active" required="false" />
                <lookupField attributeName="customerBankAccountTypeCode" required="false" />
                <lookupField attributeName="customerBankAccountTypeName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="active" />
                <field attributeName="customerBankAccountTypeCode" />
                <field attributeName="customerBankAccountTypeName" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>active</summary>
                <description>active description...</description>
			</attribute>

            <attribute name="customerBankAccountTypeCode" forceUppercase="true">
                <label>Customer Bank Account Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>customerBankAccountTypeCode</summary>
                <description>customerBankAccountTypeCode description...</description>
			</attribute>

            <attribute name="customerBankAccountTypeName" forceUppercase="true">
                <label>Customer Bank Account Type Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>customerBankAccountTypeName</summary>
                <description>customerBankAccountTypeName description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
