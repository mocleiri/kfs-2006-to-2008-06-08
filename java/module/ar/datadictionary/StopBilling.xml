<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.StopBilling">
 
        <inquiry>
            <title>Stop Billing Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="active" />
                        <field attributeName="customerStopBillingCode" />
                        <field attributeName="customerStopBillingDescription" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Stop Billing Lookup</title>

            <instructions>Look up a Stop Billing</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="customerStopBillingCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="active" required="false" />
                <lookupField attributeName="customerStopBillingCode" required="false" />
                <lookupField attributeName="customerStopBillingDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="active" />
                <field attributeName="customerStopBillingCode" />
                <field attributeName="customerStopBillingDescription" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>active</summary>
                <description>active description...</description>
			</attribute>

            <attribute name="customerStopBillingCode" forceUppercase="true">
                <label>Customer Stop Billing Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>customerStopBillingCode</summary>
                <description>customerStopBillingCode description...</description>
			</attribute>

            <attribute name="customerStopBillingDescription" forceUppercase="true">
                <label>Customer Stop Billing Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>customerStopBillingDescription</summary>
                <description>customerStopBillingDescription description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
