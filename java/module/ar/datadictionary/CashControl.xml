<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.CashControl">
 
        <inquiry>
            <title>Cash Control Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="cashControlTotalAmount" />
                        <field attributeName="customerPaymentMediumCode" />
                        <field attributeName="documentNumber" />
                        <field attributeName="referenceFinancialDocumentNumber" />
                        <field attributeName="universityFiscalPeriodCode" />
                        <field attributeName="universityFiscalYear" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Cash Control Lookup</title>

            <instructions>Look up a Cash Control</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="cashControlTotalAmount" required="false" />
                <lookupField attributeName="customerPaymentMediumCode" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="referenceFinancialDocumentNumber" required="false" />
                <lookupField attributeName="universityFiscalPeriodCode" required="false" />
                <lookupField attributeName="universityFiscalYear" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="cashControlTotalAmount" />
                <field attributeName="customerPaymentMediumCode" />
                <field attributeName="documentNumber" />
                <field attributeName="referenceFinancialDocumentNumber" />
                <field attributeName="universityFiscalPeriodCode" />
                <field attributeName="universityFiscalYear" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="cashControlTotalAmount" forceUppercase="true">
                <label>Cash Control Total Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>cashControlTotalAmount</summary>
                <description>cashControlTotalAmount description...</description>
			</attribute>

            <attribute name="customerPaymentMediumCode" forceUppercase="true">
                <label>Customer Payment Medium Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>customerPaymentMediumCode</summary>
                <description>customerPaymentMediumCode description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="referenceFinancialDocumentNumber" forceUppercase="true">
                <label>Reference Financial Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>referenceFinancialDocumentNumber</summary>
                <description>referenceFinancialDocumentNumber description...</description>
			</attribute>

            <attribute name="universityFiscalPeriodCode" forceUppercase="true">
                <label>University Fiscal Period Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>universityFiscalPeriodCode</summary>
                <description>universityFiscalPeriodCode description...</description>
			</attribute>

            <attribute name="universityFiscalYear" forceUppercase="true">
                <label>University Fiscal Year</label>
                <shortLabel>Year</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>universityFiscalYear</summary>
                <description>universityFiscalYear description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
