<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.CustomerInternalNote">
 
        <inquiry>
            <title>Customer Internal Note Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="customerInternalNoteAuthorIdentifier" />
                        <field attributeName="customerInternalNoteCreateTimestamp" />
                        <field attributeName="customerInternalNoteText" />
                        <field attributeName="customerNumber" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Customer Internal Note Lookup</title>

            <instructions>Look up a Customer Internal Note</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="customerNumber" />
                    <sortAttribute attributeName="customerInternalNoteCreateTimestamp" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="customerInternalNoteAuthorIdentifier" required="false" />
                <lookupField attributeName="customerInternalNoteCreateTimestamp" required="false" />
                <lookupField attributeName="customerInternalNoteText" required="false" />
                <lookupField attributeName="customerNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="customerInternalNoteAuthorIdentifier" />
                <field attributeName="customerInternalNoteCreateTimestamp" />
                <field attributeName="customerInternalNoteText" />
                <field attributeName="customerNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="customerInternalNoteAuthorIdentifier" forceUppercase="true">
                <label>Customer Internal Note Author Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>customerInternalNoteAuthorIdentifier</summary>
                <description>customerInternalNoteAuthorIdentifier description...</description>
			</attribute>

            <attribute name="customerInternalNoteCreateTimestamp" forceUppercase="true">
                <label>Customer Internal Note Create Timestamp</label>
                <shortLabel>Timestamp</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>customerInternalNoteCreateTimestamp</summary>
                <description>customerInternalNoteCreateTimestamp description...</description>
			</attribute>

            <attribute name="customerInternalNoteText" forceUppercase="true">
                <label>Customer Internal Note Text</label>
                <shortLabel>Text</shortLabel>

                <maxLength>400</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>customerInternalNoteText</summary>
                <description>customerInternalNoteText description...</description>
			</attribute>

            <attribute name="customerNumber" forceUppercase="true">
                <label>Customer Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>customerNumber</summary>
                <description>customerNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
