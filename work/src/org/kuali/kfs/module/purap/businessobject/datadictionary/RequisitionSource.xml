<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.RequisitionSource">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Requisition Source Inquiry</title>

            <inquiryFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="requisitionSourceCode" />
                <field attributeName="requisitionSourceDescription" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Requisition Source Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.RequisitionSource">Create New</a>]]></menubar>

            <instructions>Lookup an Requisition Source</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
                <lookupField attributeName="requisitionSourceCode" required="false" />
                <lookupField attributeName="requisitionSourceDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="requisitionSourceCode" />
                <field attributeName="requisitionSourceDescription" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="requisitionSourceCode" forceUppercase="true">
                <label>Requisition Source Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>requisitionSourceCode</summary>
                <description>requisitionSourceCode description...</description>
			</attribute>

            <attribute name="requisitionSourceDescription" forceUppercase="true">
                <label>Requisition Source Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>requisitionSourceDescription</summary>
                <description>requisitionSourceDescription description...</description>
			</attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>versionNumber</summary>
                <description>versionNumber description...</description>
			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
