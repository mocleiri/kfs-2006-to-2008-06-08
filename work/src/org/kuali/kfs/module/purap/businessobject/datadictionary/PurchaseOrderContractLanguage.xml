<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderContractLanguage">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Purchase Order Contract Language Inquiry</title>

            <inquiryFields>
                <field attributeName="campusCode" />
                <field attributeName="contractLanguageCreateDate" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="purchaseOrderContractLanguageDescription" />
                <field attributeName="purchaseOrderContractLanguageIdentifier" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Purchase Order Contract Language Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.PurchaseOrderContractLanguage">Create New</a>]]></menubar>

            <instructions>Lookup an Purchase Order Contract Language</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="FILL_IN" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="campusCode" required="false" />
                <lookupField attributeName="contractLanguageCreateDate" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
                <lookupField attributeName="purchaseOrderContractLanguageDescription" required="false" />
                <lookupField attributeName="purchaseOrderContractLanguageIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="campusCode" />
                <field attributeName="contractLanguageCreateDate" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="purchaseOrderContractLanguageDescription" />
                <field attributeName="purchaseOrderContractLanguageIdentifier" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="campusCode" forceUppercase="true">
                <label>Campus Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>campusCode</summary>
                <description>campusCode description...</description>
			</attribute>

            <attribute name="contractLanguageCreateDate" forceUppercase="true">
                <label>Contract Language Create Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>contractLanguageCreateDate</summary>
                <description>contractLanguageCreateDate description...</description>
			</attribute>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="purchaseOrderContractLanguageDescription" forceUppercase="true">
                <label>Purchase Order Contract Language Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>2000</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols=80/>
                </control>

                <summary>purchaseOrderContractLanguageDescription</summary>
                <description>purchaseOrderContractLanguageDescription description...</description>
			</attribute>

            <attribute name="purchaseOrderContractLanguageIdentifier" forceUppercase="true">
                <label>Purchase Order Contract Language Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>purchaseOrderContractLanguageIdentifier</summary>
                <description>purchaseOrderContractLanguageIdentifier description...</description>
			</attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>versionNumber</summary>
                <description>versionNumber description...</description>
			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
