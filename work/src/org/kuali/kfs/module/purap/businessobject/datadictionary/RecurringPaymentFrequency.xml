<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.RecurringPaymentFrequency">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Recurring Payment Frequency Inquiry</title>

            <inquiryFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="recurringPaymentFrequencyCode" />
                <field attributeName="recurringPaymentFrequencyDescription" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Recurring Payment Frequency Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.RecurringPaymentFrequency">Create New</a>]]></menubar>

            <instructions>Lookup a Recurring Payment Frequency</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
                <lookupField attributeName="recurringPaymentFrequencyCode" required="false" />
                <lookupField attributeName="recurringPaymentFrequencyDescription" required="false" />
             </lookupFields>

            <resultFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="recurringPaymentFrequencyCode" />
                <field attributeName="recurringPaymentFrequencyDescription" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="recurringPaymentFrequencyCode" forceUppercase="true">
                <label>Recurring Payment Frequency Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>recurringPaymentFrequencyCode</summary>
                <description>recurringPaymentFrequencyCode description...</description>
			</attribute>

            <attribute name="recurringPaymentFrequencyDescription" forceUppercase="true">
                <label>Recurring Payment Frequency Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>recurringPaymentFrequencyDescription</summary>
                <description>recurringPaymentFrequencyDescription description...</description>
			</attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>versionNumber</summary>
                <description>versionNumber description...</description>
			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
