<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.RestrictedMaterial">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Restricted Material Inquiry</title>

            <inquiryFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="restrictedMaterialCode" />
                <field attributeName="restrictedMaterialDefaultDescription" />
                <field attributeName="restrictedMaterialDescription" />
                <field attributeName="restrictedMaterialWorkgroupName" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Restricted Material Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.RestrictedMaterial">Create New</a>]]></menubar>

            <instructions>Lookup an Restricted Material</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
                <lookupField attributeName="restrictedMaterialCode" required="false" />
                <lookupField attributeName="restrictedMaterialDefaultDescription" required="false" />
                <lookupField attributeName="restrictedMaterialDescription" required="false" />
                <lookupField attributeName="restrictedMaterialWorkgroupName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="restrictedMaterialCode" />
                <field attributeName="restrictedMaterialDefaultDescription" />
                <field attributeName="restrictedMaterialDescription" />
                <field attributeName="restrictedMaterialWorkgroupName" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="restrictedMaterialCode" forceUppercase="true">
                <label>Restricted Material Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>restrictedMaterialCode</summary>
                <description>restrictedMaterialCode description...</description>
			</attribute>

            <attribute name="restrictedMaterialDefaultDescription" forceUppercase="true">
                <label>Restricted Material Default Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>restrictedMaterialDefaultDescription</summary>
                <description>restrictedMaterialDefaultDescription description...</description>
			</attribute>

            <attribute name="restrictedMaterialDescription" forceUppercase="true">
                <label>Restricted Material Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>restrictedMaterialDescription</summary>
                <description>restrictedMaterialDescription description...</description>
			</attribute>

            <attribute name="restrictedMaterialWorkgroupName" forceUppercase="true">
                <label>Restricted Material Workgroup Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>restrictedMaterialWorkgroupName</summary>
                <description>restrictedMaterialWorkgroupName description...</description>
			</attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>versionNumber</summary>
                <description>versionNumber description...</description>
			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
