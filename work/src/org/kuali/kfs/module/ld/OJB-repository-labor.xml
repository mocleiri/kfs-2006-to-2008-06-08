<class-descriptor class="org.kuali.module.labor.bo.LaborObjectBenefit" table="LD_LBR_OBJ_BENE_T">
	<field-descriptor name="universityFiscalYear" column="UNIV_FISCAL_YR" jdbc-type="INTEGER" primarykey="true" index="true"/>
	<field-descriptor name="chartOfAccountsCode" column="FIN_COA_CD" jdbc-type="VARCHAR" primarykey="true" index="true"/>
	<field-descriptor name="financialObjectCode" column="FIN_OBJECT_CD" jdbc-type="VARCHAR" primarykey="true" index="true"/>
	<field-descriptor name="financialObjectBenefitsTypeCode" column="FINOBJ_BENE_TYP_CD" jdbc-type="VARCHAR" primarykey="true" index="true"/>
	<field-descriptor name="objectId" column="OBJ_ID" jdbc-type="VARCHAR" index="true"/>
	<field-descriptor name="versionNumber" column="VER_NBR" jdbc-type="BIGINT" locking="true"/>

	<reference-descriptor name="benefitsCalculation" class-ref="org.kuali.module.labor.bo.BenefitsCalculation" auto-retrieve="true" auto-update="none" auto-delete="none">
        <foreignkey field-ref="universityFiscalYear" />
        <foreignkey field-ref="chartOfAccountsCode" />
        <foreignkey field-ref="financialObjectBenefitsTypeCode" />
	</reference-descriptor>
	
	<reference-descriptor name="financialObject" class-ref="org.kuali.module.chart.bo.ObjectCode" auto-retrieve="true" auto-update="none" auto-delete="none">
		<foreignkey field-ref="universityFiscalYear" />
		<foreignkey field-ref="chartOfAccountsCode" />
		<foreignkey field-ref="financialObjectCode" />
	</reference-descriptor>

	<reference-descriptor name="financialObjectBenefitsType" class-ref="org.kuali.module.labor.bo.BenefitsType" auto-retrieve="true" auto-update="none" auto-delete="none">
		<foreignkey field-ref="financialObjectBenefitsTypeCode" />
	</reference-descriptor>
	
	<reference-descriptor name="chartOfAccounts" class-ref="org.kuali.module.chart.bo.Chart" auto-retrieve="true" auto-update="none" auto-delete="none">
		<foreignkey field-ref="chartOfAccountsCode" />
	</reference-descriptor>
	
	<reference-descriptor name="universityFiscal"
		class-ref="org.kuali.core.bo.user.Options" auto-retrieve="true"
		auto-update="none" auto-delete="none" proxy="true">
		<foreignkey field-ref="universityFiscalYear" />
	</reference-descriptor>
</class-descriptor>

<class-descriptor class="org.kuali.module.labor.bo.BenefitsType" table="LD_BENEFITS_TYPE_T">
	<field-descriptor name="positionBenefitTypeCode" column="POS_BENEFIT_TYP_CD" jdbc-type="VARCHAR" primarykey="true" index="true"/>
	<field-descriptor name="objectId" column="OBJ_ID" jdbc-type="VARCHAR" index="true"/>
	<field-descriptor name="versionNumber" column="VER_NBR" jdbc-type="BIGINT" locking="true"/>
	<field-descriptor name="positionBenefitTypeDescription" column="POS_BENE_TYP_DESC" jdbc-type="VARCHAR"/>
	<field-descriptor name="positionBenefitRetirementIndicator" column="POS_BENE_RETIRE_CD" jdbc-type="VARCHAR" conversion="org.kuali.core.util.OjbCharBooleanConversion"/>
</class-descriptor>

<class-descriptor class="org.kuali.module.labor.bo.BenefitsCalculation" table="LD_BENEFITS_CALC_T">
	<field-descriptor name="universityFiscalYear" column="UNIV_FISCAL_YR" jdbc-type="INTEGER" primarykey="true" index="true"/>
	<field-descriptor name="chartOfAccountsCode" column="FIN_COA_CD" jdbc-type="VARCHAR" primarykey="true" index="true"/>
	<field-descriptor name="positionBenefitTypeCode" column="POS_BENEFIT_TYP_CD" jdbc-type="VARCHAR" primarykey="true" index="true"/>
	<field-descriptor name="objectId" column="OBJ_ID" jdbc-type="VARCHAR" index="true"/>
	<field-descriptor name="versionNumber" column="VER_NBR" jdbc-type="BIGINT" locking="true"/>
	<field-descriptor name="positionFringeBenefitPercent" column="POS_FRNG_BENE_PCT" jdbc-type="DECIMAL" conversion="org.kuali.core.util.OjbKualiPercentFieldConversion"/>
	<field-descriptor name="positionFringeBenefitObjectCode" column="POS_FRNGBEN_OBJ_CD" jdbc-type="VARCHAR" index="true"/>
	
	<reference-descriptor name="positionFringeBenefitObject" class-ref="org.kuali.module.chart.bo.ObjectCode" auto-retrieve="true" auto-update="none" auto-delete="none">
		<foreignkey field-ref="universityFiscalYear" />
		<foreignkey field-ref="chartOfAccountsCode" />
		<foreignkey field-ref="positionFringeBenefitObjectCode" />
	</reference-descriptor>
	
	<reference-descriptor name="chartOfAccounts" class-ref="org.kuali.module.chart.bo.Chart" auto-retrieve="true" auto-update="none" auto-delete="none">
		<foreignkey field-ref="chartOfAccountsCode" />
	</reference-descriptor>
	
	<reference-descriptor name="positionBenefitType" class-ref="org.kuali.module.labor.bo.BenefitsType" auto-retrieve="true" auto-update="none" auto-delete="none">
		<foreignkey field-ref="positionBenefitTypeCode" />
	</reference-descriptor>
	
	<reference-descriptor name="universityFiscal"
		class-ref="org.kuali.core.bo.user.Options" auto-retrieve="true"
		auto-update="none" auto-delete="none" proxy="true">
		<foreignkey field-ref="universityFiscalYear" />
	</reference-descriptor>
</class-descriptor>