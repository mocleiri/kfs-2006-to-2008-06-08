<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>KualiPaymentRequestDocumentExtractedDate</name>
      <className>org.kuali.workflow.attribute.KualiXmlSearchableAttributeImpl</className>
      <label>Kuali Payment Request Document Extracted Date Range</label>
      <description>Attribute to identify the Extracted Date Range of the Payment Request Document</description>
      <type>SearchableXmlAttribute</type>
      <searchingConfig>
        <fieldDef name="paymentRequestExtractedDate" title="kuali_dd_label(PaymentRequestDocument)">
          <display>
            <type>text</type>
          </display>
          <visibility>
            <field visible="false" />
          </visibility>
          <searchDefinition dataType="datetime" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/extractedDate')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </searchingConfig>
    </ruleAttribute>
  </ruleAttributes>
</data>
